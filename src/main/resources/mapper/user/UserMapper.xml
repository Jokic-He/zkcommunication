<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.jtedu.zkcommunication.module.user.dao.UserMapper">


    <select id="findByUsername" resultType="io.jtedu.zkcommunication.module.user.entity.UserDO">
        select * from user
        <where>
            username=#{username}
        </where>
    </select>



</mapper>